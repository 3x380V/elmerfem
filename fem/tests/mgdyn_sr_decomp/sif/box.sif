! Simple case for magnetic flux density boundary condition
! eelis.takala@gmail.com

$ B0 = 0.50
$ freq = 5
$ omega = 2 * pi * freq
$ nstep = 1
$ periodnstep = 1

Check Keywords "Warn"
INCLUDE box/mesh.names
Header 1
   Mesh DB "." "box"
End
Constants 1
   Permittivity of Vacuum = 8.8542e-12
End
Initial Condition 1
   A {e} = Real 0
   A  = Real 0
End
Simulation 1
   Max Output Level = 10
   Coordinate System = Cartesian 3D
   Coordinate Mapping(3) = 1 2 3
   Coordinate Scaling = 0.001
   Simulation Type = Transient
   Timestepping Method = BDF
   BDF Order = 1
   Timestep intervals = $ nstep * periodnstep
   Timestep Sizes = $ 10000
   Steady State Max Iterations = 1
   Output Intervals = 1
   Mesh Levels = 3
End
Solver 1  !---- MagnetoDynamics, WhitneyAVSolver
   Exec Solver = Always
   Equation = "MGDynamics"
   Variable = "A"
   Source-Reaction Decomposition = Logical True
   Procedure = "MagnetoDynamics" "WhitneyAVSolver"
   NonLinear System Relaxation Factor=1
   Export Lagrange Multiplier = Logical True
   Linear System Solver = Iterative
   Linear System Iterative Method = BicgstabL
   Linear System Robust = True
   Linear System preconditioning = none
   Linear System Convergence Tolerance = 1.e-8
   Linear System Max Iterations = 3000
   Linear System Residual Output = 1000
   Linear System ILUT Tolerance=1e-8
   BicgstabL polynomial degree = 6
   Steady State Convergence Tolerance = 1e-08

  Relative Integration Order = Integer 1
End
Solver 2  !---- MagnetoDynamics, MagnetoDynamicsCalcFields
   Exec Solver = Always
   Equation = "MGDynamicsCalc"
   Procedure = "MagnetoDynamics" "MagnetoDynamicsCalcFields"
   Linear System Symmetric = True
   Potential Variable = String "A"
   Calculate Current Density = Logical True
   Steady State Convergence Tolerance = 0
   Linear System Solver = "Iterative"
   Linear System Preconditioning = None
   Linear System Residual Output = 100
   Linear System Max Iterations = 5000
   Linear System Iterative Method = CG
   Steady State Convergence Tolerance = 1e-6
   Linear System Convergence Tolerance = 1.0e-8
End
Solver 3  !---- ResultOutputSolve, ResultOutputSolver
   Exec Solver = After timestep
   Equation = "ResultOutput"
   Procedure = "ResultOutputSolve" "ResultOutputSolver"
   Output File Name = box-d
   Vtu format = Logical True
   Save Bulk Only = True
   Save Geometry Ids = Logical True
    Discontinuous Bodies = Logical True
End
Solver 4  !---- SaveData, SaveScalars
!   Exec Solver = After timestep
   Exec Solver = Never
   Equation = "sv"
   Procedure = "SaveData" "SaveScalars"
   Filename = box/dat/box.dat
End
Equation 1
   Active Solvers(3) = 1 2 3
End
Material 1  !---- air: generic
   Name = air
   Electric Conductivity = 1
   Relative Permeability = 1
End
Body 1  !---- Air
   Name = air
   Target Bodies(1) = $ air
   Equation = 1
   Material = 1
   Initial Condition = 1
   Body Force = 1
End
Body Force 1
  Source Vector Potential 1 = Variable "Coordinate"
    Real Procedure "source" "SourceFunX2"
  Source Vector Potential 2 = Variable "Coordinate"
    Real Procedure "source" "SourceFunY2"
!  Source Vector Potential 3 = Variable "Coordinate"
!    Real Procedure "source" "SourceFunZ2"

!  Source Vector Potential 1 = Variable Coordinate 1
!    Real Procedure "source" "SourceFunX"
!  Source Vector Potential 1 = Variable Coordinate 1
!    Real Procedure "source" "SourceFunX"

!  Source Vector Potential 1 = Variable Coordinate 2
!    Real MATC "tx"
  !Source Vector Potential 2 = Variable Coordinate 1
  !  Real Procedure "source" "SourceFunY"
!  Source Vector Potential 2 = Variable Coordinate 1
!    Real MATC "0"
!  Source Vector Potential 3 = Variable Coordinate 1
!    Real MATC "tx"
  Source Potential = Real 0
End Body Force
Boundary Condition 1  !---- BCn Flux Parallel
   Name = BCn Flux Parallel
   Target Boundaries(2) = $ xplus xminus
   A {e} = Real 0
   A = Real 0
End
Boundary Condition 2  !---- BCn Flux Parallel
   Name = BCn Flux Parallel
   Target Boundaries(2) = $ yplus yminus
   A {e} = Real 0
   A = Real 0
End
Boundary Condition 3  !---- BCn Flux Parallel
   Name = BCn Flux Parallel
   Target Boundaries(2) = $ zplus zminus
   A {e} = Real 0
   A = Real 0
End
Solver 2 :: Reference Norm = Real 8.02231251E-06
Solver 2 :: Reference Norm Tolerance = Real 1E-03
